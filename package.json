{
  "name": "lmrouter-core",
  "version": "0.1.0",
  "description": "LMRouter Core",
  "homepage": "https://lmrouter.com/",
  "bugs": {
    "url": "https://github.com/LMRouter/lmrouter-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LMRouter/lmrouter-core.git"
  },
  "license": "MIT",
  "author": "LMRouter Contributors",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts config/config.yaml",
    "dev:worker": "wrangler dev --tsconfig tsconfig.worker.json",
    "build": "tsc",
    "start": "node dist/index.js config/config.yaml",
    "deploy": "wrangler deploy --minify --tsconfig tsconfig.worker.json",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings --tsconfig tsconfig.worker.json",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write ."
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@google/genai": "^1.14.0",
    "@hono/node-server": "^1.18.1",
    "hono": "^4.8.12",
    "openai": "^5.11.0",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "prettier": "^3.6.2",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3",
    "wrangler": "^4.27.0"
  }
}
